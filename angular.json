{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-shared-components": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss",
          "skipTests": true,
          "export": true,
          "flat": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/ngx-shared-components",
      "sourceRoot": "projects/ngx-shared-components/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-shared-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-shared-components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-shared-components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-shared-components/src/test.ts",
            "tsConfig": "projects/ngx-shared-components/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-shared-components/karma.conf.js"
          }
        }
      }
    },
    "shared-components-web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/shared-components-web",
      "sourceRoot": "projects/shared-components-web/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/shared-components-web",
            "index": "projects/shared-components-web/src/index.html",
            "main": "projects/shared-components-web/src/main.ts",
            "polyfills": "projects/shared-components-web/src/polyfills.ts",
            "tsConfig": "projects/shared-components-web/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/shared-components-web/src/favicon.ico",
              "projects/shared-components-web/src/assets"
            ],
            "styles": ["projects/shared-components-web/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/shared-components-web/src/environments/environment.ts",
                  "with": "projects/shared-components-web/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "shared-components-web:build:production"
            },
            "development": {
              "browserTarget": "shared-components-web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shared-components-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/shared-components-web/src/test.ts",
            "polyfills": "projects/shared-components-web/src/polyfills.ts",
            "tsConfig": "projects/shared-components-web/tsconfig.spec.json",
            "karmaConfig": "projects/shared-components-web/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/shared-components-web/src/favicon.ico",
              "projects/shared-components-web/src/assets"
            ],
            "styles": ["projects/shared-components-web/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "c40de692-3a6c-461d-83af-7df66fbc29b4"
  }
}
